{
    "name" : "goetas/twital-bundle",
    "type": "symfony-bundle",
    "description" : "Twital templating engine Symfony Bundle",
    "authors" : [{
            "name" : "Asmir Mustafic",
            "email" : "goetas@gmail.com"
        }
    ],
    "keywords" : [
        "php",
        "xml",
        "templating",
        "symfony",
        "twig"
    ],
    "homepage" : "https://github.com/goetas/twital-bundle",
    "license" : "MIT",
    "require" : {
        "goetas/twital" : "^1.1",
        "symfony/twig-bundle" : "^2.3|^3.0|^4.0",
        "php" : "^5.3.3|^7.0"
    },
    "require-dev" : {
        "symfony/framework-bundle" : "^2.3|^3.0|^4.0",
        "symfony/templating": "^2.3|^3.0|^4.0",
        "jms/translation-bundle": "^1.0",
        "phpunit/phpunit" : "^4.0",
        "kriswallsmith/assetic" : "^1.0"
    },
    "autoload" : {
        "psr-0" : {
            "Goetas\\TwitalBundle" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Goetas\\TwitalBundle\\Tests\\" : "tests/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.3-dev"
        }
    }
}
