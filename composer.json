{
    "name" : "goetas/twital-bundle",
    "description" : "Twital templating engine Symfony2 Bundle",
    "authors" : [{
            "name" : "Asmir Mustafic",
            "email" : "goetas@gmail.com"
        }
    ],
    "keywords" : [
        "php",
        "xml",
        "templating",
        "symfony",
        "twig"
    ],
    "homepage" : "https://github.com/goetas/twital-bundle",
    "license" : "MIT",
    "require" : {
        "goetas/twital" : "~1.0",
        "symfony/twig-bundle" : "~2.3|~3.0",
        "symfony/framework-bundle" : "~2.3|~3.0",
        "symfony/templating": "~2.3|~3.0",
        "php" : ">=5.3.3"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.0",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "kriswallsmith/assetic" : "~1.0"
    },
    "autoload" : {
        "psr-0" : {
            "Goetas\\TwitalBundle" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Goetas\\TwitalBundle\\Tests\\" : "tests/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    }
}
